 
@foreach (var item in Model)
{
<div class="col-12" style="background-image: url(Assets/fontend/images/demos/demo-22/slider/@item.img)">
    <div class="intro">
        <div class="title">
            <h3>@item.title</h3>
        </div>
        <div class="content">
            @Html.Raw(HttpUtility.HtmlDecode(@item.content.ToString()))
            
        </div>
        <div class="action">
            <a href="@item.action">shop now</a>
        </div>
    </div>
</div>
}
